xecution_count":9,"outputs":[]},{"cell_type":"code","source":["def contar_tokens(text, modelo=\"gpt-3.5-turbo\"):\n","  encoding = tiktoken.get_encoding(\"cl100k_base\")\n","  tokens = encoding.encode(text)\n","  return len(tokens)\n","def classify_llama(texts, output_file,chat, MAX_TOKENS_PER_REQUEST=4096, MAX_TOKENS_PER_MINUTE=60000, WAIT_TIME=60):\n","  resultados = []\n","  tokens_usados = 0  # Contador de tokens usados na janela atual\n","  # Abrindo o arquivo em modo de adição (append)\n","  classificados = set()\n","  if os.path.exists(output_file):\n","    with open(output_file, mode='r', encoding='utf-8') as file:\n","      reader = csv.reader(file)\n","      next(reader, None)  # Pula o cabeçalho\n","      classificados = {int(row[0]) for row in reader}  # Adiciona IDs classificados\n","\n","  with open(output_file, mode='a', newline='', encoding='utf-8') as file:\n","      writer = csv.writer(file)\n","\n","      # Escrevendo o cabeçalho apenas se o arquivo estiver vazio\n","      if os.stat(output_file).st_size == 0:\n","          writer.writerow([\"ID\", \"Texto\", \"Classificação\"])  # Cabeçalho\n","\n","      for i, text in enumerate(texts):\n","          try:\n","              # Contagem de tokens do texto\n","              tokens_texto = contar_tokens(text)\n","\n","              # Verifica se o texto excede o limite de tokens por requisição\n","              if tokens_texto > MAX_TOKENS_PER_REQUEST:\n","                print(f\"Texto {i+1} ignorado: excede o limite de {MAX_TOKENS_PER_REQUEST} tokens.\")\n","                continue  # Ignora esse texto e passa para o próximo\n","\n","              # Verifica se o limite de tokens foi atingido\n","              if tokens_usados + tokens_texto > MAX_TOKENS_PER_MINUTE:\n","                  print(\"Limite de tokens atingido. Aguardando para reiniciar...\")\n","                  time.sleep(WAIT_TIME)\n","                  tokens_usados = 0  # Reseta o contador de tokens\n","\n","              # Formata a mensagem usando o template\n","              formatted_message = prompt_template.format(input=text)\n","\n","              # Simulação de chamada do modelo\n","              response = chat.invoke(formatted_message)  # Ajuste conforme o método real do modelo\n","              result = response.content  # Ajuste conforme o retorno real do model\n","              print(result)\n","\n","              # Adicionando o resultado ao CSV (ID, Texto, Classificação)\n","              writer.writerow([i, text, result])\n","              resultados.append((i, text, result))\n","              tokens_usados += tokens_texto  # Atualiza os tokens usados\n","\n","              print(f\"Texto {i+1}/{len(texts)} classificado com sucesso. Tokens usados: {tokens_usados}\")\n","\n","              time.sleep(1)  # Pequeno delay entre requisições\n","\n","          except Exception as e:\n","                  error_message = str(e)\n","                  if \"rate limit\" in error_message:\n","                      # Extraia o tempo de espera da mensagem de erro\n","                      wait_time = 125  # Tempo padrão de espera (em segundos)\n","                      if \"Please try again in\" in error_message:\n","                          try:\n","                              wait_time = float(error_message.split(\"Please try again in\")[1].split(\"s\")[0])\n","                          except ValueError:\n","                              pass  # Use o tempo padrão se a extração falhar\n","\n","                      print(f\"Limite de taxa atingido. Aguardando {wait_time} segundos...\")\n","                      time.sleep(wait_time)\n","                      tokens_usados = 0  # Reseta o contador de tokens\n","                  else:\n","                      print(f\"Erro inesperado: {error_message}\")\n","                      break\n","\n","  print(\"Classificação concluída (ou interrompida) e progresso salvo.\")\n","  resultados = []\n","  tokens_usados = 0  # Contador de tokens usados na janela atual\n"],"metadata":{"id":"VUp3pqRyyFal","executionInfo":{"status":"ok","timestamp":1735436438251,"user_tz":180,"elapsed":461,"user":{"displayName":"Silas Eufrasio","userId":"04316604516393042396"}}},"execution_count":10,"outputs":[]},{"cell_type":"code","source":["def configure(model_name, temperature):\n","  config = ConfigurationLLM(model_name, temperature)\n","  return config.status()\n","\n","class ConfigurationLLM:\n","  def __init__(self, model_name, temperature):\n","    self.model_name = model_name\n","    self.temperature = temperature\n","    if model_name == \"llama-3.3-70b-versatile\" or model_name == \"llama\":\n","      os.environ[\"GROQ_API_KEY\"]=\"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\"\n","      self.chat = ChatGroq(\n","        temperature=temperature,\n","        model=\"llama-3.3-70b-versatile\"\n","      )\n","      self.OUTPUT_FILE = '/content/drive/MyDrive/TESTES_SECRETOS_TOXIC/TESTE CODIGO/classificacoes_textos.csv'\n","    if model_name == \"gpt-3.5-turbo\" or 'gpt':\n","      print(\"ui\")\n","  def status(self):\n","    return \"Programa configurado para utilização do modelo: \" + self.model_name + \"; com temperatura: \" + str(self.temperature)\n","\n","class ModelApplication:\n","    def __init__(self, config):\n","        self.config = config\n","        self.model_name = config.model_name\n","        self.chat = config.chat\n","        self.OUTPUT_FILE = config.OUTPUT_FILE\n","    def apply(self, text):\n","        if self.model_name in [\"llama-3.3-70b-versatile\", \"llama\"]:\n","          classify_llama(text, self.OUTPUT_FILE,self.chat)\n","          print('acessou o classifier')\n","        elif self.model_name == \"gpt-3.5-turbo\":\n","            # Aplicação específica para o modelo GPT-3.5\n","            print(f\"Aplicando GPT-3.5 ao texto: {text}\")\n","            # Aqui você chamaria a API do modelo GPT-3.5\n","            result = self.chat.invoke(text)  # Exemplo, pode ser diferente\n","            return result\n","        else:\n","            print(\"Modelo não reconhecido para aplicação.\")\n","            return None\n","\n","\n","\n","# Função principal para configurar e aplicar o modelo\n","def configure_and_apply(model_name, temperature, texts):\n","    # Primeiramente, configura o modelo\n","    config = ConfigurationLLM(model_name, temperature)\n","    print(config.status())  # Exibe o status da configuração\n","\n","    # Depois, aplica o modelo\n","    application = ModelApplication(config)\n","    application.apply(texts)\n","\n","    print('deu certo')\n","\n","\n","# Exemplo de uso\n","configure_and_apply(\"llama\", 0.7, textos)\n","\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"R6aPGZWKgL5R","executionInfo":{"status":"ok","timestamp":1735436622502,"user_tz":180,"elapsed":181723,"user":{"displayName":"Silas Eufrasio","userId":"04316604516393042396"}},"outputId":"0c3e5e9d-f503-4da4-f262-980c18fc2d5b"},"execution_count":11,"outputs":[{"output_type":"stream","name":"stdout","text":["ui\n","Programa configurado para utilização do modelo: llama; com temperatura: 0.7\n","None\n","Texto 1/1369 classificado com sucesso. Tokens usados: 64\n","None\n","Texto 2/1369 classificado com sucesso. Tokens usados: 151\n","None\n","Texto 3/1369 classificado com sucesso. Tokens usados: 287\n","None\n","Texto 4/1369 classificado com sucesso. Tokens usados: 302\n","None\n","Texto 5/1369 classificado com sucesso. Tokens usados: 392\n","Mocking\n","Texto 6/1369 classificado com sucesso. Tokens usados: 400\n","Mocking\n","Texto 7/1369 classificado com sucesso. Tokens usados: 412\n","Ironic\n","Texto 8/1369 classificado com sucesso. Tokens usados: 425\n","Vulgarity\n","Texto 9/1369 classificado com sucesso. Tokens usados: 576\n","Vulgarity\n","Texto 10/1369 classificado com sucesso. Tokens usados: 593\n","Bitter Frustration\n","Texto 11/1369 classificado com sucesso. Tokens usados: 626\n","Bitter Frustration\n","Texto 12/1369 classi